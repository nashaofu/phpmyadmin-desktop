{
  "name": "phpmyadmin-desktop",
  "version": "0.0.1",
  "description": "phpMyAdmin 的桌面版本",
  "main": "dist/main/app.js",
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "start": "electron .",
    "dev": "npm run dev:main & npm run dev:renderer",
    "dev:main": "webpack --config build/main/webpack.config.dev.js",
    "dev:renderer": "webpack-dev-server --config build/renderer/webpack.config.dev.js",
    "build:main": "webpack --config build/main/webpack.config.prod.js",
    "build:renderer": "webpack --config build/renderer/webpack.config.prod.js",
    "build": "npm run build:main & npm run build:renderer",
    "pack": "electron-builder",
    "release": "electron-builder",
    "lint": "eslint . --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/phpmyadmin-desktop.git"
  },
  "keywords": [
    "phpMyAdmin",
    "Desktop"
  ],
  "author": "nashaofu",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/nashaofu/phpmyadmin-desktop/issues"
  },
  "homepage": "https://github.com/nashaofu/phpmyadmin-desktop#readme",
  "dependencies": {
    "decompress": "^4.2.1",
    "detect-port": "^1.3.0",
    "fs-extra": "^9.0.0",
    "log4js": "^6.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "electron-dev-webpack-plugin": "^1.0.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
